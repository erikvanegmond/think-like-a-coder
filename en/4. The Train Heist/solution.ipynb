   "source": [
    "position = 10\n",
    "train = Train()\n",
    "for move in train.move():\n",
    "    print(move)\n",
    "    if move == \"->\":\n",
    "        position = position - 1\n",
    "    else:\n",
    "        position = position + 1\n",
    "\n",
    "    if position == 0:\n",
    "        train.open_force_field()\n",
    "        break\n"
