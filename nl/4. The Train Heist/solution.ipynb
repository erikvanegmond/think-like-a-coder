   "source": [
    "wagon = 10\n",
    "trein = Trein()\n",
    "for beweeg in trein.beweeg():\n",
    "    print(beweeg)\n",
    "    if beweeg == \"->\":\n",
    "        wagon = wagon - 1\n",
    "    else:\n",
    "        wagon = wagon + 1\n",
    "\n",
    "    if wagon == 0:\n",
    "        trein.open_force_field()\n",
    "        break\n"
   ]
